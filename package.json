{
  "name": "use-common-hook",
  "version": "2.0.0.alpha.0",
  "description": "Lightweight collection of generic hooks + primitives that should be in react by default",
  "main": "dist/use-common-hook.cjs",
  "module": "dist/use-common-hook.module.js",
  "umd:main": "dist/use-common-hook.umd.js",
  "unpkg": "dist/use-common-hook.min.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XantreGodlike/use-common-hook.git"
  },
  "scripts": {
    "test": "vitest",
    "build": "prettier --write src/**/* && rimraf dist && microbundle ./src/index.ts",
    "npmpublish": "na publish --no-git-checks",
    "testpublish": "na publish --dry-run .",
    "mypublish": "nr build && nr npmpublish"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0 || ^18.0.0"
  },
  "keywords": [
    "react",
    "react hooks",
    "generic hooks",
    "common hooks"
  ],
  "author": "XantreGodlike",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "jsdom": "^20.0.3",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "vitest": "^0.25.5"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
